{
  "compilerOptions": {
    "lib": ["es2023"],
    "target": "es2023",
    "module": "esnext",
    "rootDirs": ["src"],
    "outDir": "dist",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "types": ["node"],
    "skipLibCheck": true,
    "strict": true,
  },
    "include": ["src", "api"],
    "exclude": ["node_modules", "dist"]
}